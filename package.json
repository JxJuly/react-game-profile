{
  "name": "@july_cm/react-game-profile",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./psn": {
      "types": "./dist/types/psn/index.d.ts",
      "require": "./dist/psn/index.js",
      "import": "./dist/psn/index.js"
    },
    "./psn/*": {
      "import": "./dist/psn/*"
    },
    "./steam": {
      "types": "./dist/types/steam/index.d.ts",
      "require": "./dist/steam/index.js",
      "import": "./dist/steam/index.js"
    },
    "./steam/*": {
      "import": "./dist/steam/*"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/main/index.js",
      "import": "./dist/main/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/main/index.js",
  "module": "./dist/main/index.js",
  "scripts": {
    "dev": "vite -c ./config/vite-dev.config.ts",
    "build": "vite build -c ./config/vite-build.config.ts"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "psn-api": "^2.11.2",
    "steamwebapi-ts": "^1.10.0"
  },
  "devDependencies": {
    "@july_cm/eslint-config": "^2.3.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.23.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^19"
  }
}